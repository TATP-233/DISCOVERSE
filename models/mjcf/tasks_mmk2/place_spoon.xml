      <geom size="0.02 0.02 0.375" pos=" 0.3 -0.55 0.375" type="box" contype="0" conaffinity="0" rgba="0.8 0.8 0.8 1" />
      <geom size="0.02 0.02 0.375" pos="-0.3 -0.55 0.375" type="box" contype="0" conaffinity="0" rgba="0.8 0.8 0.8 1" />
    </body>
  </worldbody>

  <actuator>
    <include file="../mmk2/mmk2_control.xml"/>
  </actuator>

  <sensor>
    <include file="../mmk2/mmk2_sensor.xml"/>
  </sensor>

</mujoco>