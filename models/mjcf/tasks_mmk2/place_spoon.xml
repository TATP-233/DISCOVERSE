      <joint type="free" frictionloss="0.0002"/>
      <include file="../object/bowl_pink.xml"/>
    </body>

    <!-- ======= spoon ======= -->
    <body name="spoon" pos=" 0.529063 0.0915262 0.760495" quat="0.6 0 0 0.7">
      <joint type="free" frictionloss="0.0002"/>
      <include file="../object/spoon.xml"/>
