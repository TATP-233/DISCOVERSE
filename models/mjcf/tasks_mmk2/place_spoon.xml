    <body name="table" pos="0.65 0 0">
      <geom size="0.35 0.6 0.01" pos="0. 0. 0.75" type="box" rgba="0.8 0.8 0.8 1" condim="4" solimp="2 1 0.01" solref="0.01 1" friction="1 0.005 0.0001"/>
